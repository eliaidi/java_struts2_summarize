		return "success";
	}
	
	public String update(){
		
		System.out.println("update");
			
		return "success";
	}
	
	public String delete(){
		
		System.out.println("delete");
		
		return "success";
	}
	
	public String query(){
		
		System.out.println("query");
		
		return "success";
	}

}
